generator client {
  provider = "prisma-client-js"
  output   = "../generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Movie {
  id        Int    @id @default(autoincrement())
  name      String
  wokeScore Int
  wokeMeter Int
  summary   String
  headline  String
  poster    String
}

model MoviePossibleNames {
  id           Int    @id @default(autoincrement())
  possibleName String
  name         String
}
